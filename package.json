{
  "name": "playGo",
  "version": "0.0.1",
  "description": "Play Go on Messenger Chat",
  "author": "Samping Chuang",
  "private": true,
  "dependencies": {
    "babel-polyfill": "6.9.0",
    "body-parser": "1.15.1",
    "express": "4.13.4",
    "immutable": "3.8.1",
    "invariant": "2.2.1",
    "knox": "0.9.2",
    "mysql2": "1.4.2",
    "request": "2.72.0",
    "sharp": "0.17.2",
    "sequelize": "4.8.0",
    "weiqi": "0.2.0",
    "lwip": "0.0.9",
    "node-polyglot": "2.0.0",
    "fb-local-chat-bot": "0.1.42",
    "st": "1.2.2",
    "locks": "0.2.2",
    "text-to-svg": "3.1.0",
    "deep-equal": "1.0.1"
  },
  "engines": {
    "node": "5.12.0",
    "npm": "2.15.5"
  },
  "scripts": {
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "watch": "babel --watch=./src --out-dir=./build --source-maps inline --copy-files",
    "build": "babel ./src --out-dir=./build --source-maps inline --copy-files",
    "s": "nodemon --expose-gc build/app.js --watch build",
    "sdev": "NODE_ENV=local_dev nodemon --expose-gc build/app.js --watch build",
    "sprod": "NODE_ENV=local_prod nodemon --expose-gc build/app.js --watch build",
    "d": "node debug build/app.js",
    "web_watch": "NODE_ENV='dev' webpack --watch --watch-polling --config public/src/webpack.config.js",
    "web_p_watch": "NODE_ENV='prod' webpack -p --watch --watch-polling --config public/src/webpack.config.js",
    "test": "NODE_ENV=test mocha --compilers js:babel-register",
    "lint": "eslint src --fix",
    "check": "npm run lint && npm run flow && npm run test",
    "migrate_local": "sequelize db:migrate --config db/config.json --migrations-path db/migrations --env local",
    "migrate_dev": "sequelize db:migrate --config db/config.json --migrations-path db/migrations --env development --debug",
    "migrate_undo_dev": "sequelize db:migrate:undo --config db/config.json --migrations-path db/migrations --env development",
    "migrate_prod": "sequelize db:migrate --config db/config.json --migrations-path db/migrations --env production --debug",
    "migrate_undo_prod": "sequelize db:migrate:undo --config db/config.json --migrations-path db/migrations --env production",
    "ai_server_dev": "gunicorn --bind 0.0.0.0:8000 -t 3000 gochatai.server:app --worker-class=gevent",
    "gen_models": "node build/scripts/auto-gen-model-classes.js"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.8.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-async-to-module-method": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "6.9.0",
    "bluebird": "3.4.1",
    "bower": "^1.7.9",
    "chai": "^3.5.0",
    "classnames": "2.2.5",
    "cli-table": "0.3.1",
    "esformatter": "0.10.0",
    "esformatter-flow": "1.0.1",
    "eslint": "^2.10.2",
    "eslint-config-defaults": "9.0.0",
    "express-handlebars": "3.0.0",
    "flow-bin": "0.52.0",
    "jquery": "2.2.4",
    "mocha": "2.5.3",
    "nodemon": "^1.9.2",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "request-promise": "4.1.1",
    "sequelize-cli": "3.1.0",
    "smartgame": "0.1.4",
    "sqlite3": "3.1.4",
    "supertest": "1.2.0",
    "supertest-as-promised": "3.1.0",
    "webpack": "^1.13.0",
    "recharts": "1.0.0-beta.2"
  },
  "optionalDependencies": {
    "fsevents": "^1.0.6"
  }
}
